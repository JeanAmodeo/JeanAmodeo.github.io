  div.boton-2 {
    margin:0 auto 150px 0;
    width: 290px;
    height: 70px;
    font-size: 24px;
    font-weight: 400;
    text-align: center;
    overflow: hidden;
    position: relative;
  }
  div.boton-2 a {
    color: #333;
    border: none;
    text-decoration: none;
    -webkit-transition: all 600ms ease-in-out;
    -moz-transition: all 600ms ease-in-out;
    -ms-transition: all 600ms ease-in-out;
    -o-transition: all 600ms ease-in-out;
    transition: all 600ms ease-in-out;
  }
  div.boton-2:hover a {
    color: #333;
    border: none;
    text-decoration: none;
  }
  div.boton-2 span {
    font-family: Abril Fatface;
    width: 100%;
    position: absolute;
    top:15px;
    left:0;
  }

  div.boton-2 svg line {
    stroke-width: 6;
    stroke: #333;
    fill: none;

    -webkit-transition: all 300ms ease-in-out;
    -moz-transition: all 300ms ease-in-out;
    -ms-transition: all 300ms ease-in-out;
    -o-transition: all 300ms ease-in-out;
    transition: all 300ms ease-in-out;
  }
  div.boton-2:hover svg line {
    stroke: #333;
  }

  div.boton-2:hover svg line.L-T {
    -webkit-transform: translateY(-20px);
    transform: translateY(-20px);
  }
  div.boton-2:hover svg line.L-B {
    -webkit-transform: translateY(-50px);
    transform: translateY(-50px);
  }
  div.boton-2:hover svg line.R-T {
    -webkit-transform: translateY(50px);
    transform: translateY(50px);
  }
  div.boton-2:hover svg line.R-B {
    -webkit-transform: translateY(20px);
    transform: translateY(20px);
  }
  div.boton-2:hover svg line.T-L {
    -webkit-transform: translateX(270px);
    transform: translateX(270px);
  }
  div.boton-2:hover svg line.T-R {
    -webkit-transform: translateX(20px);
    transform: translateX(20px);
  }
  div.boton-2:hover svg line.B-L {
    -webkit-transform: translateX(-20px);
    transform: translateX(-20px);
  }
  div.boton-2:hover svg line.B-R {
    -webkit-transform: translateX(-270px);
    transform: translateX(-270px);
  }


